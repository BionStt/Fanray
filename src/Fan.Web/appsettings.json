{
  "AppSettings": {
    "Version": "1.0.0-alpha",
    "Database": "sqlite",
    "PreferredDomain": "auto",
    "UseHttps": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\sql2016;Database=Fanray;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.RollingFile", "Serilog.Sinks.Seq" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "./logs/log-{Date}.txt" }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}
