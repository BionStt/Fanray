{
  "AppSettings": {
    // Media Storage
    // true: Azure Blob Storage will be used; false: file system will be used.
    "UseBlobStorage": false,
    // Azure Blob Storage connection string, default to development storage in dev, requires emulator https://docs.microsoft.com/en-us/azure/storage/common/storage-use-emulator
    "AzureBlobStorageConnectionString": "UseDevelopmentStorage=true"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=Fanray;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.RollingFile",
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "./logs/log-{Date}.txt"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}