{
  "Database": {
    "UseSqLite": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\sql2016;Database=Fanray;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.RollingFile" ],
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "./logs/log-{Date}.txt" }
      }
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
