    <rule name="www" enabled="true" stopProcessing="true" lockItem="true">
      <match url="(.*)" />
      <conditions>
        <add input="{HTTP_HOST}" pattern="!^www\." ignoreCase="true" />
      </conditions>
      <action type="Redirect" url="http://www.{HTTP_HOST}{REQUEST_URI}" appendQueryString="true" redirectType="Permanent" />
    </rule>
